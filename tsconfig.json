{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "target": "ES2020",
    "module": "ES2020",
    "jsx": "react-jsx",
    "strict": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "types": ["react", "react-native"],
    "skipLibCheck": true,
    "lib": ["es2020", "dom"]
  },
  "include": ["src", "App.tsx"],
  "exclude": ["node_modules", "build", "dist"]
}
